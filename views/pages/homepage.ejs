<%- include( '../partials/header') %>
<div class="homepage">
    <h2>All Facespace members</h2>
<ul class="homepage__all">
    <% users.forEach(user => { %>
    <li class="homepage__all--user">
      <% if (user._id === currentUser.friends) { %>
        <a href=/users/<%= user._id %>><img src="<%= user.avatarUrl %>" /></a>
        <p>Friend</p>
      <% } else { %>
        <a href=/users/<%= user._id %>><img src="<%= user.avatarUrl %>" /></a>
      <% } %>
    </li>
    <% }) %>
  </ul>
</div>
<%- include( '../partials/footer') %>